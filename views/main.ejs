<html>
	<head>
		<title></title>
		<link rel="stylesheet" href="/css/materialize.css">
	</head>
	<body>
		<div class="container">
			<div class="row">
				<h2>Here are all the quotes!</h2>
			</div>
		</div>
		<div class="container">
			<% for(x in quotes){ %>
				<div class='quote'>
					<h5><%= quotes[x].name %></h5>
					<p><%= quotes[x].quote %></p>
				</div>
			<% } %>
		</div>
		<div class="container">
			<div class="row">
				<a href="/">Go back!</a>
			</div>
		</div>
	</body>
</html>